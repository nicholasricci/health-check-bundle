{
    "name": "ekreative/health-check-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A bundle that provides a simple /healthcheck route",
    "keywords": ["healthcheck", "doctrine", "redis"],
    "autoload": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "src/Ekreative/HealthCheckBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "tests/Ekreative/HealthCheckBundle/"
        },
        "classmap": [
            "tests/TestKernel.php"
        ]
    },
    "require": {
        "ext-redis": "*",
        "php": ">=5.4",
        "symfony/framework-bundle": "^2.8|^3.4|^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^2.8|^3.4|^4.0",
        "doctrine/doctrine-bundle": "1.6.4",
        "phpunit/phpunit": "4.8.*",
        "friendsofphp/php-cs-fixer": "2.2.*",
        "symfony/monolog-bundle": "3.2.*",
        "symfony/browser-kit": "^2.8|^3.4|^4.0",
        "symfony/yaml": "^2.8|^3.4|^4.0",
        "sensio/framework-extra-bundle": "v3.0.*"
    },
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ]
}
